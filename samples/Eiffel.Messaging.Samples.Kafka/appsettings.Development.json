{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Messaging": {
    "Kafka": {
      "ProducerConfig": {
        "BootstrapServers": "localhost:9092",
        "CompressionType": 3,
        "Partitioner": 1
      },
      "ConsumerConfig": {
        "BootstrapServers": "localhost:9092",
        "GroupId": "Eiffel.Messaging.Samples.Kafka",
        "PartitionAssignmentStrategy": 1,
        "EnableAutoOffsetStore": "True",
        "AutoCommitIntervalMs": 0
      }
    }
  }
}
